{
  "name": "reviews",
  "version": "",
  "description": "This is the module on an ecommerce page that shows reviews of the store",
  "author": "",
  "license": "",
  "main": "server/index.js",
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "engines": {
    "node": ">=6.13.0"
  },
  "scripts": {
    "server": "nodemon server/index.js",
    "react-dev": "webpack -d --watch",
    "create-db": "mysql  --defaults-extra-file=./config.txt < schema.sql",
    "rollback": "node_modules/.bin/knex migrate:rollback",
    "migrations": "node_modules/.bin/knex migrate:latest",
    "seed": "node_modules/.bin/knex seed:run",
    "seed-db": "npm run create-db && npm run migrations && npm run seed",
    "test": "jest --verbose --watchAll --coverage",
    "update": "jest --updateSnapshot"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "faker": "^4.1.0",
    "knex": "^0.16.3",
    "moment": "^2.24.0",
    "mysql": "^2.16.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "sprintf-js": "^1.1.2",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "eslint-config-hackreactor": "git://github.com/reactorcore/eslint-config-hackreactor",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "react-test-renderer": "^16.8.2",
    "webpack": "^4.29.2",
    "webpack-cli": "^3.2.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  }
}
